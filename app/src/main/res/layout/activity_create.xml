<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/just"
            style="@style/tv"
            android:text="just" />

        <TextView
            android:id="@+id/fromArray"
            style="@style/tv"
            android:text="fromArray" />

        <TextView
            android:id="@+id/fromIterable"
            style="@style/tv"
            android:text="fromIterable" />

        <TextView
            android:id="@+id/fromCallable"
            style="@style/tv"
            android:text="fromCallable" />

        <TextView
            android:id="@+id/fromFuture"
            style="@style/tv"
            android:text="fromFuture" />

        <TextView
            android:id="@+id/defer"
            style="@style/tv"
            android:text="defer" />

        <TextView
            android:id="@+id/timer"
            style="@style/tv"
            android:text="timer" />

        <TextView
            android:id="@+id/interval"
            style="@style/tv"
            android:text="interval" />

        <TextView
            android:id="@+id/intervalRange"
            style="@style/tv"
            android:text="intervalRange" />

        <TextView
            android:id="@+id/range"
            style="@style/tv"
            android:text="range" />

        <TextView
            android:id="@+id/empty_never_error"
            style="@style/tv"
            android:text="empty/never/error" />
    </LinearLayout>
</ScrollView>